{
  "name": "blockprotocol-poc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch src/server.ts",
    "dev:frameworks": "cross-env NODE_ENV=development ENABLE_FRAMEWORK_WATCH=true tsx watch src/server.ts",
    "dev:once": "cross-env TSX_NO_CACHE=1 NODE_ENV=development tsx src/server.ts",
    "dev:once-frameworks": "cross-env TSX_NO_CACHE=1 NODE_ENV=development ENABLE_FRAMEWORK_WATCH=true tsx src/server.ts",
    "build": "rimraf dist && cross-env NODE_ENV=production tsc -p tsconfig.server.json && vite build",
    "build:standalone": "tsc --project tsconfig.standalone.json",
    "start": "cross-env NODE_ENV=production node dist/server/server.js",
    "start:standalone": "tsx src/standalone-server.ts",
    "test": "playwright test",
    "test:devserver": "tsx --test tests-node/dev-server-smoke.test.ts",
    "build:frameworks": "tsx scripts/build-frameworks.ts",
    "scaffold": "tsx scripts/scaffold-block.ts"
  },
  "dependencies": {
    "@blockprotocol/core": "0.1.3",
    "@blockprotocol/graph": "0.3.4",
    "@vivafolio/block-loader": "file:../../packages/block-loader",
    "block-template": "^0.0.26",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@playwright/test": "1.52.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/compiler-sfc": "^3.4.0",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "solid-js": "^1.8.0",
    "svelte": "^4.2.0",
    "terser": "^5.30.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.0",
    "vite": "^5.2.0",
    "vue": "^3.4.0"
  }
}
